<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_article_frame_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.openclassrooms.go4lunch.MainActivity">

    <TextView
        android:id="@+id/message"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:textSize="32dp"
        android:text="COUCOU"/>

    <Button
        android:id="@+id/map_activity_button_logout_google"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/logout_google"
        android:textColor="@color/white"
        android:textSize="@dimen/default_text_size"
        android:textAllCaps="false"
        android:background="@drawable/button_radius_google"
        style="?android:attr/borderlessButtonStyle"
        android:layout_marginTop="100dp"
        android:padding="@dimen/default_large_margin"
        android:elevation="2dp"
        android:drawableStart="@drawable/ic_google" />
</FrameLayout>
